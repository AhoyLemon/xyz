mixin clientSiteSummary(s)

  .project.client-site
    figure
      if s.screenshots
        .screenshots.slick-slider
          each shot in s.screenshots
            img(src="img/client-sites/"+shot)
    figcaption
      .title
        a(href=s.url)= s.title
      if s.client
        .subhead= s.client
      if s.description
        | !{s.description}
      if s.problem
        .problem
          h3 The Problem
          | !{s.problem}
      if s.solution
        .solution
          h3 The Solution
          | !{s.solution}

    if s.tech || s.cms
      .tech
        each tech in s.tech
          .tooltip(data-tippy-content=tech)
            +techIcon(tech)
        .tooltip(data-tippy-content=s.cms)
          +techIcon(s.cms)